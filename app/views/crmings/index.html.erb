<p id="notice"><%= notice %></p>

<h1>Crmings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @crmings.each do |crming| %>
      <tr>
        <td><%= link_to 'Show', crming %></td>
        <td><%= link_to 'Edit', edit_crming_path(crming) %></td>
        <td><%= link_to 'Destroy', crming, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Crming', new_crming_path %>
